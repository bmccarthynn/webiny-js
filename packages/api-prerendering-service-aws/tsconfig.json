{
  "extends": "../../tsconfig.json",
  "include": ["src"],
  "references": [
    { "path": "../api-prerendering-service" },
    { "path": "../handler" },
    { "path": "../handler-args" },
    { "path": "../plugins" }
  ],
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./dist",
    "declarationDir": "./dist",
    "paths": {
      "~/*": ["./src/*"],
      "@webiny/api-prerendering-service/*": ["../api-prerendering-service/src/*"],
      "@webiny/api-prerendering-service": ["../api-prerendering-service/src"],
      "@webiny/handler/*": ["../handler/src/*"],
      "@webiny/handler": ["../handler/src"],
      "@webiny/handler-args/*": ["../handler-args/src/*"],
      "@webiny/handler-args": ["../handler-args/src"],
      "@webiny/plugins/*": ["../plugins/src/*"],
      "@webiny/plugins": ["../plugins/src"]
    },
    "baseUrl": "."
  }
}
