{
  "extends": "../../tsconfig",
  "include": ["./src", "./__tests__"],
  "references": [
    {
      "path": "../api-tenancy"
    },
    {
      "path": "../db-dynamodb"
    },
    {
      "path": "../error"
    },
    {
      "path": "../plugins"
    }
  ],
  "compilerOptions": {
    "paths": {
      "~/*": ["./src/*"],
      "@webiny/api-tenancy/*": ["../api-tenancy/src/*"],
      "@webiny/api-tenancy": ["../api-tenancy/src"],
      "@webiny/db-dynamodb/*": ["../db-dynamodb/src/*"],
      "@webiny/db-dynamodb": ["../db-dynamodb/src"],
      "@webiny/error/*": ["../error/src/*"],
      "@webiny/error": ["../error/src"]
    },
    "baseUrl": "."
  }
}
