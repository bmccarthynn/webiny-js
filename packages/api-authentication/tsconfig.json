{
  "extends": "../../tsconfig",
  "include": ["./src", "./__tests__"],
  "references": [
    {
      "path": "../handler"
    },
    {
      "path": "../handler-http"
    }
  ],
  "compilerOptions": {
    "paths": {
      "~/*": ["./src/*"],
      "@webiny/handler/*": ["../handler/src/*"],
      "@webiny/handler": ["../handler/src"],
      "@webiny/handler-http/*": ["../handler-http/src/*"],
      "@webiny/handler-http": ["../handler-http/src"]
    },
    "baseUrl": "."
  }
}
